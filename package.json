{
  "name": "jbj-playground",
  "version": "1.0.0",
  "private": true,
  "description": "JBJ Playground: test your stylesheets online",
  "main": "index.html",
  "scripts": {
    "build": "browserify --debug -r jbj -r jbj-array -r jbj-parse -r jbj-template -r jbj-rdfa -r jbj-nlp -r jbj-numerical -r jbj-jsonld -r reqwest > ./assets/js/jbj.js",
    "start": "http-server",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Inist-CNRS/jbj-playground.git"
  },
  "keywords": [
    "JSON",
    "jbj",
    "online",
    "playground",
    "stylesheet"
  ],
  "author": "Fran√ßois Parmentier <francois.parmentier@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Inist-CNRS/jbj-playground/issues"
  },
  "homepage": "https://github.com/Inist-CNRS/jbj-playground#readme",
  "dependencies": {
    "browserify": "^13.0.1",
    "http-server": "^0.8.5",
    "jbj": "^6.0.2",
    "jbj-array": "^1.0.0",
    "jbj-jsonld": "^1.0.0",
    "jbj-nlp": "^1.1.0",
    "jbj-numerical": "^1.0.0",
    "jbj-parse": "^2.0.0",
    "jbj-rdfa": "^1.1.2",
    "jbj-template": "^2.0.0",
    "reqwest": "^2.0.5"
  }
}
